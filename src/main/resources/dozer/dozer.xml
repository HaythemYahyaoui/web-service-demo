<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping map-id="developer-update" wildcard="false">
		<class-a>com.lansrod.test1.entity.Developer</class-a>
		<class-b>com.lansrod.test1.entity.Developer</class-b>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>lastname</a>
			<b>lastname</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
	</mapping>

	<mapping map-id="language-update" wildcard="false">
		<class-a>com.lansrod.test1.entity.Language</class-a>
		<class-b>com.lansrod.test1.entity.Language</class-b>
		<field>
			<a>label</a>
			<b>label</b>
		</field>
	</mapping>


</mappings>  
        